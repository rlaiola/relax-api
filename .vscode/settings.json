    "shellcheck": {
      "args": {
        "$zsh": {
          "extensions": [".zsh"],
          "shebangs": [
            "/bin/zsh",
            "/usr/bin/zsh",
            "/usr/local/bin/zsh",
            "/usr/bin/env zsh"
          ]
        }
      },
      "capabilities": [],
      "command": ["shellcheck", "--format", "json", "--enable", "all", "-"],
      "configFiles": [],
      "enabled": true,
      "languages": ["shellscript", "bash"],
      "name": "shellcheck",
      "url": "https://github.com/koalaman/shellcheck",
      "when": ["!$zsh"]
    },
